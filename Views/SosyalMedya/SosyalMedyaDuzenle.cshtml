@model MVC_Cv.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "SosyalMedyaDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Sosyal Medya Düzenleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Sosyal Medya ")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.İkon, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Güncelle</button>
</form>

